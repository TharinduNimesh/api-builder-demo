    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.5.3",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1",
